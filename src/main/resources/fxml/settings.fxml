<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.mfxcore.controls.Label?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="settingsView" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0">
      <stylesheets>
         <URL value="@../css/settings.css"/>
         <URL value="@../css/dark.css"/>
      </stylesheets>
      <Label text="Einstellungen">
         <styleClass>
            <String fx:value="font-size-title"/>
            <String fx:value="font-style"/>
            <String fx:value="label"/>
         </styleClass>
         <VBox.margin>
            <Insets left="20.0" top="10.0"/>
         </VBox.margin>
      </Label>
   </VBox>
</AnchorPane>
